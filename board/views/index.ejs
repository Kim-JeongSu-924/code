<html>

<head>
    <title></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

    <script>
        function moveBoard(no) {
            location.href = "/board/" + no;
        }
    </script>
</head>

<body>
    <table class="table">
        <thead>
            <tr>
                <th>글번호</th>
                <th>제목</th>
                <th>조회수</th>
                <th>작성자</th>
                <th>작성시간</th>
            </tr>
        </thead>
        <tbody>
            <% list.forEach(function(board){ %>
                <tr onclick="moveBoard(<%=board.no%>)">
                    <td>
                        <%=board.no%>
                    </td>
                    <td>
                        <%=board.title%>
                    </td>
                    <td>
                        <%=board.hits%>
                    </td>
                    <td>
                        <%=board.writer%>
                    </td>
                    <td>
                        <%=moment(board.writeDate).format("YYYY-MM-DD HH:mm") %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>


    <div class="text-end">
        <a href="/write" class="btn btn-primary">글쓰기</a>
    </div>


</body>

</html>